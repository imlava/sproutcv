{
  "name": "sproutcv-enhanced",
  "version": "3.0.0",
  "description": "Ultra-Advanced AI Resume Analyzer with Vector Search",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "db:generate": "supabase gen types typescript --local",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "functions:deploy": "supabase functions deploy",
    "analyze": "npm run lint && npm run type-check && npm run test",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@supabase/supabase-js": "^2.39.1",
    "@supabase/ssr": "^0.1.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.2",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "lucide-react": "^0.302.0",
    "clsx": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "framer-motion": "^10.16.16",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "recharts": "^2.8.0",
    "react-use": "^17.4.2",
    "use-debounce": "^10.0.0",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.8",
    "@types/react-window": "^1.8.8",
    "fuse.js": "^7.0.0",
    "compromise": "^14.10.0",
    "natural": "^6.7.0",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "jszip": "^3.10.1",
    "file-saver": "^2.0.5",
    "@types/file-saver": "^2.0.7",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
    "sharp": "^0.33.1",
    "crypto-js": "^4.2.0",
    "@types/crypto-js": "^4.2.1",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.7",
    "jose": "^5.1.3",
    "iron-session": "^8.0.1"
  },
  "devDependencies": {
    "@next/eslint-config-next": "^14.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@playwright/test": "^1.40.1",
    "supabase": "^1.123.4",
    "@supabase/cli": "^1.123.4",
    "next-sitemap": "^4.2.3",
    "@next/bundle-analyzer": "^14.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run analyze"
    }
  },
  "keywords": [
    "ai",
    "resume",
    "analyzer",
    "vector-search",
    "gemini",
    "supabase",
    "next.js",
    "typescript",
    "career",
    "job-matching",
    "ats",
    "recruitment"
  ],
  "author": "SproutCV Team",
