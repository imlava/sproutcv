{
  "name": "sproutcv-enhanced",
  "version": "2.0.0",
  "description": "Enterprise-grade AI resume analysis platform with Document AI, Vertex embeddings, and advanced features",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "functions:serve": "supabase functions serve",
    "functions:deploy": "supabase functions deploy",
    "deploy:enhanced": "supabase functions deploy enhanced-gemini-analyzer && supabase functions deploy event-processor",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "setup:env": "cp .env.example .env.local",
    "setup:gcp": "node scripts/setup-google-cloud.js",
    "setup:complete": "npm run setup:env && npm run setup:gcp && npm run db:migrate"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.15.0",
    "@google-cloud/documentai": "^8.4.0",
    "@google-cloud/dlp": "^5.2.0",
    "@google-cloud/pubsub": "^4.0.7",
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.12.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-badge": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-textarea": "^1.0.5",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.38.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "google-auth-library": "^9.4.0",
    "lucide-react": "^0.303.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "supabase": "^1.142.2"
  },
  "keywords": [
    "AI",
    "resume",
    "analysis",
    "ATS",
    "job-search",
    "document-ai",
    "vertex-ai",
    "nextjs",
    "supabase",
    "typescript",
    "enterprise"
